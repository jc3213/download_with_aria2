{
    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_description__",
    "version": "3.8.0",
    "manifest_version": 3,
    "permissions": [
        "contextMenus",
        "downloads",
        "tabs",
        "cookies",
        "notifications",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "default_locale" : "en",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "24": "icons/icon24.png"
        },
        "default_popup": "popup/index.html"
    },
    "icons": {
        "128": "icons/icon128.png",
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
    },
    "options_ui": {
        "page": "options/index.html",
        "open_in_tab": false
    }
}
